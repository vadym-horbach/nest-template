{
  "name": "nest-template",
  "version": "1.0.0",
  "description": "",
  "author": "Vadym Horbach",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\" --config ./.prettierrc",
    "lint": "eslint  -c .eslintrc.js \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json",
    "typeorm": "npx typeorm-ts-node-commonjs -d ./src/models/ormconfig.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/migrations/Migration",
    "migration:create": "npm run typeorm -- migration:create src/migrations/Migration",
    "migration:up": "npm run typeorm -- migration:run",
    "migration:down": "npm run typeorm -- migration:revert"
  },
  "engines": {
    "node": ">=16.15.1"
  },
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@fastify/helmet": "^10.0.2",
    "@fastify/static": "^6.5.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.1.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.6",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-fastify": "^9.1.6",
    "@nestjs/platform-socket.io": "^9.1.6",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.1.6",
    "@socket.io/redis-adapter": "^7.2.0",
    "@socket.io/redis-emitter": "^4.1.1",
    "accept-language-parser": "^1.5.0",
    "aws-sdk": "^2.1246.0",
    "bignumber.js": "^9.1.0",
    "cache-manager": "^5.1.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "fastify-multer": "^2.0.3",
    "handlebars": "^4.7.7",
    "ioredis": "^5.2.4",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nest-winston": "^1.8.0",
    "nestjs-i18n": "^9.1.5",
    "nestjs-throttler-storage-redis": "^0.1.20",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "pg-query-stream": "^4.2.4",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "sharp": "^0.31.1",
    "socket.io": "^4.5.3",
    "typeorm": "^0.3.10",
    "winston": "^3.8.2",
    "winston-cloudwatch": "^6.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.6",
    "@types/accept-language-parser": "^1.5.3",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.0",
    "@types/jest": "28.1.6",
    "@types/lodash": "^4.14.182",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.11.9",
    "@types/nodemailer": "^6.4.6",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.3",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
